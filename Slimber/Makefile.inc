NIM_TARGET = Nim/nim
NIM_SOURCES = \
	Nim/main.cpp
NIM_OBJECTS = \
	$(NIM_SOURCES:.cpp=.o) \

.PHONY: nim
nim: $(NIM_TARGET)

$(NIM_TARGET): $(SWIFTEN_TARGET) $(NIM_OBJECTS)
	$(QUIET_LINK)$(CXX) -o $(NIM_TARGET) $(NIM_OBJECTS) $(LDFLAGS) $(SWIFTEN_TARGET) $(LIBS)

#TARGETS += $(NIM_TARGET)
CLEANFILES += $(NIM_OBJECTS)
DEPS += \
	$(NIM_SOURCES:.cpp=.dep) \
